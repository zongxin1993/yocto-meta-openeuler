sysfs /sys sysfs defaults 0 0
none /var tmpfs nodev,nosuid,mode=755,noexec 0 0
none /var/log/audit tmpfs nodev,nosuid,mode=755 0 0
devtmpfs /dev devtmpfs defaults 0 0
tmpfs /dev/shm tmpfs nodev,nosuid,mode=1777,noexec 0 0
devpts /dev/pts devpts mode=620 0 0
tmpfs /tmp tmpfs nodev,nosuid,noexec 0 0
